{
  "name": "tiamo",
  "version": "0.1.0",
  "description": "Typescript inborn AWS DynamoDB ORM",
  "repository": "https://github.com/vitarn/tiamo",
  "author": "colder",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "jest --runInBand",
    "testing": "jest --runInBand --watch",
    "coverage": "jest --coverage --runInBand",
    "typecheck": "tsc --noEmit",
    "release": "standard-version",
    "prepack": "tsc --removeComments --inlineSourceMap false | echo shut up"
  },
  "dependencies": {
    "tdv": "^0.1.2"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/joi": "^13.0.6",
    "@types/node": "^9.4.7",
    "aws-sdk": "^2.211.1",
    "dynalite": "^1.2.0",
    "jest": "^22.4.2",
    "joi": "12",
    "standard-version": "^4.3.0",
    "ts-jest": "^22.4.2",
    "typescript": "^2.7.2"
  },
  "peerDependencies": {
    "aws-sdk": "*"
  },
  "files": [
    "**/**.js",
    "**/*.d.ts",
    "!__tests__"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(t)sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
